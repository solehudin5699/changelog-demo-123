{
  "name": "changelog",
  "version": "1.5.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solehudin5699/changelog-demo-123"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nick-devs/001-add-a-changelog-to-any-project/issues"
  },
  "homepage": "https://github.com/nick-devs/001-add-a-changelog-to-any-project#readme",
  "dependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@release-it/conventional-changelog": "^5.0.0",
    "husky": "^8.0.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "release: release v${version}",
      "requireUpstream": false,
      "tagName": "v${version}",
      "requireBranch": "master"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {}
          ]
        }
      }
    }
  },
  "devDependencies": {
    "release-it": "^15.10.3"
  }
}
